{
  "expo": {
    "name": "Conference Room Booking",
    "slug": "CRB",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rikardoricz.confereceroombooking",
      "versionCode": 1
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./src/assets/icon.png",
          "dark": {
            "image": "./src/assets/splash.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d1b08076-b59e-49d5-bd46-2455dd3731e7"
      }
    },
    "ios": {
      "bundleIdentifier": "com.rikardoricz.confereceroombooking"
    }
  }
}
